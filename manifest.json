{
	"name": "JavaScript Blacklist",
	"version": "0.1",
	"description": "Blacklist JavaScript from certain domains",
	"background_page": "background.html",
	"options_page": "options.html",
	"content_scripts": [{
		"matches": ["http://*/*", "https://*/*"],
		"run_at": "document_start",
		"js": ["jsblacklist.js"]
	}],
	"icons": {
		"48": "icons/icon48.png",
		"128": "icons/icon128.png"
	}
}
